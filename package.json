{
  "name": "react-issue-tracker",
  "version": "1.0.0",
  "description": "issue tracker built using react and postgreSQL",
  "main": "src/index.ts",
  "repository": "https://github.com/shihabdider/react-issue-tracker.git",
  "author": "Shihab Dider <shihabdider@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0",
    "@types/node": "^14.11.2"
  },
  "scripts": {
    "start": "nodemon --exec ts-node --files src/index.ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "_moduleDirectories": [
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
        "yarn fix",
        "git add"
    ]
  }
}
